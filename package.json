{
  "name": "nodejs-express-sequelize-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Any Node.js Project who wants to use any SQL DB with Sequelize-Express.js",
  "main": "server.js",
  "scripts": {
    "start": "nodemon -e js,ejs --max-http-header-size 15000 --max-old-space-size=2048 clusteredApp.js",
    "restart": "lsof -i:8080 | grep -v 'PID' | awk '{print $2}' | xargs kill -9 && nodemon --max-http-header-size 15000 --max-old-space-size=2048 app.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "Node.js",
    "Sequelize",
    "SQL",
    "Express"
  ],
  "author": "Franklin P. Thaker",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^26.6.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-yaml": "^0.4.2",
    "jest": "^26.6.3",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "farmhash": "^3.2.1",
    "husky": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "simple-crypto-js": "^3.0.0",
    "socket.io": "^4.1.1",
    "uuid": "^8.3.2"
  }
}
